<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CC000000"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    android:layout_marginLeft="0dp"
	android:layout_marginTop="0dp" >

    <ImageView
        android:id="@+id/menu_detail_big_img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="70dp" />

    <Button
        android:id="@+id/menu_detail_btn_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="630dp"
        android:layout_marginTop="20dp"
        android:background="#00000000"
        android:textSize="24sp"
        android:textColor="#FFFFFFFF"
        android:text="@string/menu_detail_btn_close" />
    
    <Button
        android:id="@+id/menu_detail_btn_addorder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="630dp"
        android:layout_marginTop="300dp"
        android:background="#00000000"
        android:textSize="24sp"
        android:textColor="#FFFFFFFF"
        android:text="@string/menu_detail_btn_addorder" />

    <TextView
        android:id="@+id/menu_detail_lab_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="375dp"
        android:textSize="26sp"
        android:textColor="#FF999999" />
    
    <TextView
        android:id="@+id/menu_detail_lab_price"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="240dp"
        android:layout_marginTop="375dp"
        android:textSize="26sp"
        android:textColor="#FF999999" />

</RelativeLayout>
